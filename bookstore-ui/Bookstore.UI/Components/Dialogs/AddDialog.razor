@inherits BaseDialog 

<MudDialog>
     <TitleContent>
         <MudText Typo="Typo.h6" Align="Align.Center">
             Add @EntityName
         </MudText>
     </TitleContent>
     <DialogActions>
         <MudButton Color="Color.Default" Variant="Variant.Filled" OnClick="@Cancel">Cancel</MudButton>
         <MudButton Color="Color.Info" Variant="Variant.Filled" OnClick="@Confirm">Save</MudButton>
     </DialogActions>
 </MudDialog>
